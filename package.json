{
  "name": "node-tutorial",
  "version": "1.0.0",
  "description": "A simple Node.js tutorial to use the Mail API",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production forever start --uid=\"outlook\" -a server/app.js",
    "dev": "cross-env NODE_ENV=development node server/app.js",
    "install:dev": "npm install && bower install",
    "install:prod": "npm install --production"
  },
  "keywords": [
    "outlook",
    "microsoft"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "bower": "^1.8.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.3",
    "csv-parser": "^1.11.0",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "forever": "^0.15.3",
    "lodash": "^4.17.0",
    "moment": "^2.16.0",
    "multiparty": "^4.1.2",
    "node-outlook": "^1.1.6",
    "simple-oauth2": "^0.7.0",
    "util": "^0.10.3"
  },
